#server.ssl.key-password = acumos
#server.ssl.key-alias=secure-server
server.ssl.trust-store = src/main/resources/server-truststore.jks
server.ssl.trust-store-password = acumos
server.ssl.client-auth = need

logging.level.org.springframework.security: DEBUG


